{
    "TitleBar": {
        "ID": "App_TitleBar"
        , "CONTENT": "<div id=\"App_TitleText\">{TITLE}</div><div id=\"App_TitleButtons\">{WINDOW_BUTTONS}</div>"
        , "Settings": {
            "bUseButton_Minimize": true
            , "bUseButton_Maximize": true
            , "bUseButton_Close": true
        }
    },
    "Header": {
        "ID": "App_Header"
        , "CONTENT": ""
        , "CONTENT_OLD": "<div id=\"notifyArea\"></div><div id=\"app_HeaderLoc\" class=\"left\"></div><div id=\"app_HeaderApps\" class=\"center\"></div><div class=\"right\"></img><img src=\"img/refresh.png\" onclick=\"location.reload();\" title=\"Reload (F5)\"></img></div>"
        , "OLD": "<img src=\"img/settings.png\" onclick=\"wzWND('Settings').refresh();\" title=\"Settings (F6)\">"
    },
    "Nav": {
        "ID": "App_Navigation",
        "CONTENT": "myNav",
        "Items":{
            "News":{
                "Change Log": []
            }
            , "Filter":{
                "Settings":[
                    ".Manage Tags"
                    , ".Manage Groups"
                ]
                , ".Library":[
                    ".Special Highlighting"
                    , ".Mastery Marker"
                ]
            }
        },
        "Disabled":{
            "Docs":{
                "ReadMe": [],
                "Change Log": [],
                "Wiki":["Mastery Wizard","Skill Edit - Window"]
            }
        }
    },
    "Content": {
        "ID": "App_Content",
        "CONTENT": "myBody"
    },
    "SideBar": {
        "ID": "App_SideBar",
        "CONTENT": "mySideBar"
    },
    "Footer": {
        "ID": "App_Footer",
        "hasConsole": true,
        "tpl": {
            "console": "<div id=\"footerConsole\"><div class=\"header\"></div></div>",
            "body": "<div id=\"footerBody\"><div class=\"left\">v{VERSION}<span id=\"installUpdate\">checking for Updates...</span></div><div class=\"center\">{COPYRIGHT}</div><div class=\"right\">{CONTACT} | WanezProjects Discord Server: <span class=\"Link\" title=\"Opens link in Browser on click.\" onclick=\"require('electron').shell.openExternal(`https://discord.gg/ru6eU2p`)\">https://discord.gg/ru6eU2p</span></div></div>"
            , "oldUpdate": "<span id=\"installUpdate\">checking for Updates...</span>"
        }
    }
}